{
  "name": "cjhms_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build": "cd android && gradlew assembleRelease",
    "menu": "adb shell input keyevent 82",
    "iconfontMap": "node ./src/scripts/iconfontMap/iconfontMap.js",
    "remove": "adb uninstall com.cjhms_app",
    "stop": "adb shell am force-stop com.cjhms_app",
    "start-app": "adb shell am start -n com.cjhms_app/.MainActivity",
    "clear-build-cache": "cd android && gradlew clean",
    "clear-build": "rimraf android/app/build",
    "clear": "npm run clear-build-cache && npm run clear-build-cache",
    "clear-all": "yarn cache clean -f && npm cache clean -f && cnpm cache clean -f && npm run clear"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.7.8",
    "immer": "^1.10.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "ramda": "^0.26.1",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-orientation": "^3.1.3",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^8.0.10",
    "react-native-svg-uri": "^1.2.3",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.9",
    "react-navigation-redux-helpers": "^2.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.2",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3",
    "shelljs": "^0.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
